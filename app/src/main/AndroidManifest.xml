<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.yangke.java">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".v.BaseApp"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/BaseTheme">
        <activity android:name=".v.me.SoftwareDescActivity" />     <!--软件说明-->
        <activity android:name=".v.search.SearchResultActivity" /> <!-- 搜索结果页 -->
        <activity android:name=".v.base.BaseActivity" />           <!-- Activity Base -->
        <activity android:name=".v.wxapi.WXEntryActivity" />       <!-- 微信分享 -->
        <activity
            android:name=".v.MainActivity"
            android:windowSoftInputMode="adjustPan|adjustResize">  <!-- 主页面 -->
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>